{
  "name": "vue-auto-import-checker",
  "version": "0.11.0",
  "description": "Check if the used components in templates are all registered within your components.d.ts",
  "main": "index.ts",
  "scripts": {
    "test": "vitest run --reporter=tree",
    "test:watch": "vitest --reporter=tree",
    "lint": "oxlint --tsconfig=tsconfig.json",
    "tsc": "tsc --noEmit",
    "build": "tsc --outDir ./dist"
  },
  "engines": {
    "node": ">=20.19.1",
    "npm": ">=10.8.2"
  },
  "bin": {
    "vue-auto-import-checker": "cli.ts"
  },
  "keywords": [
    "vue",
    "vuetify",
    "vuuse",
    "quasar",
    "vue-router",
    "unplugin",
    "components.d.ts",
    "tags"
  ],
  "author": "Marcel Wagner (https://github.com/marcelwagner)",
  "contributors": [
    {
      "name": "Chi Nguyen",
      "url": "https://github.com/cnguyen-de"
    },
    {
      "name": "Marcus Moedinger",
      "url": "https://github.com/mreise"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/marcelwagner/vue-auto-import-checker#readme",
  "bugs": {
    "url": "https://github.com/marcelwagner/vue-auto-import-checker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelwagner/vue-auto-import-checker.git"
  },
  "type": "module",
  "devDependencies": {
    "@vueuse/components": "13.5.0",
    "oxlint": "^1.28.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "quasar": "1.15.8",
    "vitest": "^4.0.14",
    "vuetify": "3.8.3"
  },
  "dependencies": {
    "@types/node": "24.10.1",
    "commander": "14.0.2",
    "dotenv": "17.2.3",
    "html-tags": "5.1.0",
    "svg-tag-names": "3.0.1",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "winston": "3.18.3"
  }
}
