{
  "name": "vue-auto-import-checker",
  "version": "0.7.1",
  "description": "Check if the used vue components in templates are all registered with unplugin",
  "main": "index.ts",
  "scripts": {
    "lint": "oxlint --tsconfig=tsconfig.json"
  },
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=10.8.2"
  },
  "bin": {
    "vue-auto-import-checker": "cli.ts"
  },
  "keywords": [
    "vue",
    "unplugin",
    "vue3",
    "vue2"
  ],
  "author": "Marcel Wagner <marcel.wagner@email.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelwagner/vue-auto-import-checker"
  },
  "type": "module",
  "devDependencies": {
    "@vueuse/components": "13.5.0",
    "oxlint": "^1.28.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "vuetify": "3.8.3"
  },
  "dependencies": {
    "@types/node": "24.10.1",
    "commander": "14.0.2",
    "dotenv": "17.2.3",
    "html-tags": "5.1.0",
    "svg-tag-names": "3.0.1",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
  }
}
